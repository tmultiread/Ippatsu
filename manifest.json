{
  "manifest_version": 2,
  "name": "IppatsuAlt",
  "version": "1.1",

  "description": "Ippatsu Alternate extension",
  "icons": {
    "16": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["tabs", "*://*.xvideos.com/video*"]
}
